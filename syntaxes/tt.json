{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Phenoscript",
  "fileTypes": [
    "phs"
  ],
  "scopeName": "source.phs",
  "patterns": [
    {
      "comment": "modules",
      "match": "(mod)\\s+((?:r#(?!crate|[Ss]elf|super))?[a-z][A-Za-z0-9_]*)"
    }
  ]
}
